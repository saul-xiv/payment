{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/payment/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "paymentservice",
          "Port": 5000
        }
      ],
      "UpstreamPathTemplate": "/api/payment/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PATCH" ]
    },
    {
      "DownstreamPathTemplate": "/swagger/v1/swagger.json",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "paymentservice",
          "Port": 5000
        }
      ],
      "UpstreamPathTemplate": "/payment/swagger/v1/swagger.json",
      "UpstreamHttpMethod": [ "GET" ]
    },
    {
      "DownstreamPathTemplate": "/api/login/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "loginservice",
          "Port": 5002
        }
      ],
      "UpstreamPathTemplate": "/api/login/{everything}",
      "UpstreamHttpMethod": [ "POST" ]
    },
    {
      "DownstreamPathTemplate": "/swagger/v1/swagger.json",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "loginservice",
          "Port": 5002
        }
      ],
      "UpstreamPathTemplate": "/login/swagger/v1/swagger.json",
      "UpstreamHttpMethod": [ "GET" ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5000"
  }
}
